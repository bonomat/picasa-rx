{
  "name": "picasa-rx",
  "version": "1.0.0",
  "description": "A Picasa WebClient written in ES2015 using rxJS Observables",
  "repository": {
    "type": "git",
    "url": "https://github.com/bonomat/picasa-rx.git"
  },
  "license": "MIT",
  "options": {
    "mocha": "--require scripts/mocha_runner src/**/__tests__/**/*.test.js"
  },
  "scripts": {
    "prepublish": ". ./scripts/prepublish.sh",
    "lint": "eslint ./src",
    "lintfix": "eslint ./src --fix",
    "testonly": "mocha $npm_package_options_mocha",
    "test": "npm run lint && npm run testonly",
    "test-watch": "npm run testonly -- --watch --watch-extensions js"
  },
  "devDependencies": {
    "@kwonoj/rxjs-testscheduler-compat": "0.0.5",
    "babel-cli": "6.x.x",
    "babel-core": "6.x.x",
    "babel-eslint": "^6.1.2",
    "babel-plugin-rewire": "^1.0.0",
    "babel-plugin-transform-runtime": "6.x.x",
    "babel-polyfill": "6.x.x",
    "babel-preset-es2015": "6.x.x",
    "babel-preset-stage-2": "6.x.x",
    "chai": "^3.4.1",
    "eslint": "^3.18.0",
    "eslint-plugin-babel": "^4.1.x",
    "mocha": "^2.3.3",
    "nodemon": "1.10.x",
    "sinon": "^1.17.2",
    "sinon-chai": "^2.8.0"
  },
  "dependencies": {
    "babel-runtime": "6.x.x",
    "querystring": "^0.2.0",
    "request": "^2.81.0",
    "rxjs": "^5.2.0"
  }
}
